{
  "name": "bpd-storage",
  "version": "0.4.0",
  "description": "Simple storage api wrapper",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "build:minify": "webpack --config webpack.build.minify.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "karma start karma.config.js",
    "prod": "npm run build && npm run build:esm && npm run build:minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpd-d/bpd-storage.git"
  },
  "keywords": [
    "bpd",
    "storage"
  ],
  "author": "bpd_dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpd-d/bpd-storage/issues"
  },
  "homepage": "https://github.com/bpd-d/bpd-storage#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "clean-webpack-plugin": "^3.0.0",
    "jasmine": "^3.6.1",
    "karma-typescript": "^5.2.0",
    "serialize-javascript": "^5.0.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "typescript-transform-paths": "^2.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-karma-jasmine": "^4.0.0",
    "webpack-merge": "^4.2.2"
  }
